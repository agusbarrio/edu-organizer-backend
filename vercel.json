{
  "installCommand": "npm run install:migrate",
  "buildCommand": "npm run vercel-build && npm run migrate",
  "devCommand": "npm run dev",
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ]
}
